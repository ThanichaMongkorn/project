#include<iostream>
#include<vector>
using namespace std;

// ฟังก์ชันนับคะแนนรวมจากการจับคู่ไพ่
int total_score(vector cards) {
int sum = 0;
for (int i = 0; i < cards.size() - 1; i += 2) {
int score = // เรียกใช้ฟังก์ชันจับคู่ไพ่
sum += score;
}
return sum;
}

int main() {
vector cards = {5, 5, 10, 7, 3, 3, 6, 9};

// เรียกใช้ฟังก์ชัน total_score เพื่อนับคะแนนรวมจากการจับคู่ไพ่
int score = total_score(cards);

cout << "total score =  " << score << endl;
return 0;
}